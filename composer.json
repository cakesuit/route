{
    "name": "cakesuit/route",
    "description": "Cakesuit/Route plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Frédéric KOLLER",
            "email": "labelsuisse@gmail.com"
        }
    ],
    "require": {
        "cakephp/cakephp": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Cakesuit\\Route\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cakesuit\\Route\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
